#ifndef DRIVERS_DRV_GPIO_HPP_
#define DRIVERS_DRV_GPIO_HPP_

/* MCU Include */
#include "stm32f3xx.h"

/* Typedef Structure */
typedef struct
{
	uint32_t	Pin;
	uint32_t	Mode;
	uint32_t 	Speed;

}GPIOInitStructure_t;


/* GPIOx Driver Class */
class HAL_GPIO
{
public:
	void	init(GPIO_TypeDef *GPIOx, GPIOInitStructure_t *GPIOx_cfg);

private:

};


#endif /* DRIVERS_DRV_GPIO_HPP_ */
