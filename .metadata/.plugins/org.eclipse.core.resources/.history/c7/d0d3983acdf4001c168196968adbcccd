/* Include Header File */
#include "Drivers/drv_gpio.hpp"

/*
 *
 *
 *
 */
void HAL_GPIO::init(GPIO_TypeDef *GPIOx, GPIOInitStructure_t *GPIOx_cfg)
{

	/* Set GPIOx Mode */
	GPIOx->MODER |= (GPIOx_cfg->Mode << GPIOx_cfg->Pin);

}
